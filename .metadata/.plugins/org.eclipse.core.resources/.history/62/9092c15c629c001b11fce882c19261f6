package com.callor.game.model;

public class DeckVO {

	//카드는 조커를 제외하고 52장 
	
	private String cardName[];
	private Character cardMark[];
	
	
	private int cardScore[] ;
	
	
	public DeckVO() {

		cardName = new String[52];
		cardMark = new Character[] {"◆", "♥", "♣", "♠" } ;
		
		cardScore = new int[52];
		
	}
}

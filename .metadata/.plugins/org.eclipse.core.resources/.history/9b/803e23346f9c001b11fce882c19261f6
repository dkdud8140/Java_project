package com.callor.game.model;

import java.util.ArrayList;
import java.util.List;
import java.util.Random;

public class DealerDeckVO {

	DeckVO deckVO ;

	protected Random rnd;

	public DealerDeckVO() {
		deckVO = new DeckVO() ;
		rnd = new Random();
	}

	public void firstdDeck() {
		List<String> dDeckCard = new ArrayList<String>;
		List<Integer> dDeckScore = new ArrayList<Integer>;
		
		int intCard1 = rnd.nextInt(52);
		int intCard2 ;
		
		while (true) {
			intCard2 = rnd.nextInt(52);
			if (intCard2 == 0) {
				continue;
			}
			break ;
		}
		
		String card1 = deckVO.getCardName(intCard1);
		String card2 = deckVO.getCardName(intCard2);
		
		int score1 = deckVO.getCardScore(intCard1);
		int score2 = deckVO.getCardScore(intCard2);
		
		
	}

}
